{
    "name": "node-red-meeseeks-perf",
    "description": "A Node-RED Project",
    "version": "0.0.1",
    "dependencies": {
        "node-red": "3.0.2",
        "node-red-contrib-fs": "~1.4.1",
        "node-red-contrib-google-sheets": "~1.1.2",
        "node-red-dashboard": "~3.2.0",
        "node-red-node-ui-list": "~0.3.6",
        "node-red-contrib-counter": "~0.1.6",
        "dashboard-evi": "~1.0.2",
        "node-red-node-random": "~0.4.0",
        "node-red-contrib-moving-average": "~1.3.0",
        "node-red-contrib-kafka-client": "0.0.10",
        "node-red-contrib-kafka-node": "~0.1.7",
        "node-red-contrib-mqtt-dynamicsub": "0.0.9",
        "node-red-contrib-redis": "~1.3.9",
        "node-red-node-redis": "0.0.5",
        "node-red-node-base64": "~0.3.0",
        "node-red-contrib-mgrs": "~0.1.0"
    },
        "scripts": {
        "start": "node $NODE_OPTIONS node_modules/node-red/red.js $FLOWS",
        "debug": "node --inspect=0.0.0.0:9229 $NODE_OPTIONS node_modules/node-red/red.js $FLOWS",
        "debug_brk": "node --inspect=0.0.0.0:9229 --inspect-brk $NODE_OPTIONS node_modules/node-red/red.js $FLOWS"
    },
    "node-red": {
        "settings": {
            "flowFile": "flows.json",
            "credentialsFile": "flows_cred.json"
        }
    }
}
